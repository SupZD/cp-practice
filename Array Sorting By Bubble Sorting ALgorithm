#include <stdio.h>
int main()
{
    int a[100],i,j,n;
    printf("Enter the number of elements: ");
    scanf("%d",&n);
    printf("Enter The elements: ");
    for (i=0;i<n;i++)
    {
        printf("\na[%d]=",i);
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for (j=i+1;j<n;j++)
        {
            if(a[i]>a[j])
            {
                 int temp=a[i];
                 a[i]=a[j];
                 a[j]=temp;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        printf("\na[%d]=%d",i,a[i]);
    }
    return 0;
}
