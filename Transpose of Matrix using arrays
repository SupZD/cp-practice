#include <stdio.h>

int main() {
    int a[100][100], b[100][100];
    int i, j, arows, acolumns;

    printf("Enter rows and columns of your matrix: ");
    scanf("%d%d", &arows, &acolumns);

    printf("Enter the elements of your matrix:\n");
    for(i = 0; i < arows; i++) {
        for(j = 0; j < acolumns; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Transpose logic
    for(i = 0; i < arows; i++) {
        for(j = 0; j < acolumns; j++) {
            b[j][i] = a[i][j]; 
        }
    }

    printf("The transpose is:\n");
    for(i = 0; i < acolumns; i++) {  
        for(j = 0; j < arows; j++) {
            printf("%d ", b[i][j]);
        }
        printf("\n");
    }

    return 0;
}
